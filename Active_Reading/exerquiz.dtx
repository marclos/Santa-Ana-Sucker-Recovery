{\count0=\value{tocdepth}
\advance\count0by2 \xdef\eqbmkmrkdepth{\the\count0}}
\@ifundefined{Hy@bookmarksdepth}
    {\setcounter{tocdepth}{\eqbmkmrkdepth}}
    {\hypersetup{bookmarksdepth=\eqbmkmrkdepth}}
